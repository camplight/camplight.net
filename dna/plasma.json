{
  "BundleCode": {
    "source": "node_modules/organic-webcell/plasma/BundleCode",
    "root": "/client/",
    "useCache": true,
    "debug": false,
    "uglify": true,
    "plugins": [{
      "source": "/plasma/BundleCode-addEnv"
    }]
  },
  "RenderPage": {
    "source": "node_modules/organic-webcell/plasma/RenderPage",
    "root": "/templates/",
    "useCache": true
  },
  "MountHttpPages": {
    "source": "plasma/MountHttpPages",
    "cwd": {
      "pages": "pages",
      "pageActions": "page-helpers"
    }
  },
  "ErrorHandler": {
    "source": "node_modules/organic-webcell/plasma/ErrorHandler"
  }
}